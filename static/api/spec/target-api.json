        "description": "Mencari data nama dan nomor laboratorium yang terdaftar dalam sistem NAR<br>Pencarian ini hanya menampilkan maksimum 10 laboratorium yang sesuai kriteria pencarian. Jika anda tidak menemukan laboratorium yang anda cari, perbaiki kriteria pencarian anda menjadi lebih spesifik.",
        "operationId": "CariLAB",
        "parameters": [
          {
            "in": "query",
            "required": true,
            "name": "q",
            "description": "Kriteria pencarian, diisi dengan potongan kata dalam pencarian, pencarian ini akan mencari nama dan kode lab.",
            "type": "string"
          }
        ],
        "security": [
          {
            "ApiKeyAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Berhasil",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/InfoLab"
              }
            }
          }
        }
      }
    },
    "/lov/cariFasKes": {
      "get": {
        "tags": [
          "List Of Values"
        ],
        "summary": "Mencari data nama dan nomor Fasilitas Kesehatan",
        "description": "Mencari data nama dan nomor Faskes yang terdaftar dalam sistem NAR<br>Pencarian ini hanya menampilkan maksimum 10 rumah sakit dan 10 puskesmas yang sesuai kriteria pencarian. Jika anda tidak menemukan faskes yang anda cari, perbaiki kriteria pencarian anda menjadi lebih spesifik.",
        "operationId": "CariFasKes",
        "parameters": [
          {
            "in": "query",
            "required": true,
            "name": "q",
            "description": "Kriteria pencarian, diisi dengan potongan kata dalam pencarian, pencarian ini akan mencari nama dan kode faskes.",
            "type": "string"
          }
        ],
        "security": [
          {
            "ApiKeyAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Berhasil",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/DataFaskes"
              }
            }
          }
        }
      }
    }
  },
  "securityDefinitions": {
    "ApiKeyAuth": {
      "type": "apiKey",
      "in": "header",
      "name": "X-API-KEY"
    }
  },
  "definitions": {
    "HasilInputDataSpesimen": {
      "type": "object",
      "properties": {
        "idSpesimen": {
          "type": "string",
          "description": "Auto increment ID data spesimen yang diciptakan oleh NAR"
        }
      }
    },
    "HasilInputBatchSpesimen": {
      "type": "object",
      "properties": {
        "status": {
          "type": "string",
          "enum": [
            "SUKSES",
            "GAGAL"
          ],
          "description": "Status keberhasilan, SUKSES atau GAGAL"
        },
        "info": {
          "type": "string",
          "description": "Berisi IDSpesimen jika sukses, atau Alasan jika gagal"
        }
      }
    },
    "DetilDataSpesimen": {
      "type": "object",
      "required": [
        "pasien",
        "faskes",
        "spesimen"
      ],
      "properties": {
        "inputOleh": {
          "type": "string",
          "description": "User yang melakukan input data"
        },
        "idSpesimen": {
          "type": "string",
          "description": "Auto increment ID data spesimen yang diciptakan oleh NAR"
        },
        "pasien": {
          "$ref": "#/definitions/DetilDataPasien"
        },
        "faskes": {
          "$ref": "#/definitions/DataFaskes"
        },
        "spesimen": {
          "$ref": "#/definitions/DataSpesimenWithID"
        },
        "hasil": {
          "$ref": "#/definitions/HasilSpesimen"
        }
      }
    },
    "InputDataSpesimen": {
      "type": "object",
      "required": [
        "spesimen"
      ],
      "properties": {
        "inputOleh": {
          "type": "string",
          "description": "User yang melakukan input data"
        },
        "spesimen": {
          "$ref": "#/definitions/DataSpesimen"
        },
        "pasien": {
          "$ref": "#/definitions/DataPasien"
        },
        "faskes": {
          "$ref": "#/definitions/DataFaskes"
        },
        "hasil": {
          "$ref": "#/definitions/HasilSpesimen"
        }
      }
    },
    "DetilDataPasien": {
      "type": "object",
      "allOf": [
        {
          "$ref": "#/definitions/DataPasien"
        }
      ],
      "properties": {
        "idPasien": {
          "type": "string",
          "description": "Auto increment ID data pasien yang diciptakan oleh NAR"
        }
      }
    },
    "DataPasien": {
      "type": "object",
      "required": [
        "nama",
        "tanggalLahir",
        "jenisIdentitas",
        "nomorIdentitas",
        "alamatDomisili"
      ],
      "properties": {
        "nama": {
          "type": "string",
          "description": "Nama pasien, wajib disertakan",
          "example": "Budi Sutopo Mihardjo"
        },
        "jenisIdentitas": {
          "type": "integer",
          "enum": [
            1,
            2
          ],
          "description": "1=KTP, 2=PASSPORT"
        },
        "nomorIdentitas": {
          "type": "string",
          "description": "Nomor KTP 16 digit atau passport 7-9 digit yang valid.",
          "example": "1234567890123456"
        },
        "tanggalLahir": {
          "type": "string",
          "description": "Tanggal lahir pasien, dengan format yyyy-mm-dd",
          "example": "1982-03-12"
        },
        "usiaTahun": {
          "type": "integer",
          "description": "Umur pasien dalam tahun",
          "example": 34
        },
        "usiaBulan": {
          "type": "integer",
          "description": "Umur pasien dalam bulan yang ditambahkan dalam field usiaTahun tahun. Sebagai contoh:<ul><li>Untuk seorang bayi berumur 5 bulan, maka usiaTahun=0 dan usiaBulan=5</li><li>Untuk seorang balita berumur 15 bulan, maka usiaTahun=1 dan usiaBulan=3</li></ul>",
          "example": 9
        },
        "jenisKelamin": {
          "type": "integer",
          "enum": [
            1,
            2
          ],
          "description": "Jenis kelamin pasien. 1=Laki-laki, 2=Perempuan"
        },
        "wargaNegara": {
          "type": "integer",
          "enum": [
            1,
            2
          ],
          "description": "Status kewarganegaraan, 1=WNI(Warga Negara Indonesia), 2=WNA(Warga Negara Asing)"
        },
        "ketWargaNegara": {
          "type": "string",
          "description": "Catatan mengenai status kewarganegaraan"
        },
        "alamatDomisili": {
          "$ref": "#/definitions/DataAlamat"
        }
      }
    },
    "DataAlamat": {
      "required": [
        "kelurahan",
        "kecamatan",
        "kabupatenKota",
        "provinsi"
      ],
      "type": "object",
      "properties": {
        "nomorTelepon": {
          "type": "string",
          "description": "Nomor telepon atau HP yang bisa dihubungi",
          "example": "08776543210"
        },
        "alamatDomisili": {
          "type": "string",
          "description": "Informasi alamat tempat tinggal sekarang",
          "example": "Jl. Irian Blok A/12"
        },
        "rt": {
          "type": "string",
          "example": "002",
          "description": "Nomor rukun tetangga"
        },
        "rw": {
          "type": "string",
          "example": "003",
          "description": "Nomor rukun warga"
        },
        "kelurahan": {
          "$ref": "#/definitions/NamaKode"
        },
        "kecamatan": {
          "$ref": "#/definitions/NamaKode"
        },
        "kabupatenKota": {
          "$ref": "#/definitions/NamaKode"
        },
        "provinsi": {
          "$ref": "#/definitions/NamaKode"
        }
      }
    },
    "DataSpesimenWithID": {
      "type": "object",
      "allOf": [
        {
          "$ref": "#/definitions/DataSpesimen"
        }
      ],
      "properties": {
        "idSpesimen": {
          "type": "string",
          "description": "Auto increment ID spesimen yang diciptakan oleh NAR"
        }
      }
    },
    "DataSpesimen": {
      "required": [
        "nomorSpesimen"
      ],
      "type": "object",
      "properties": {
        "tujuanPemeriksaan": {
          "type": "integer",
          "enum": [
            0,
            1,
            2,
            3,
            4,
            5
          ],
          "description": "Tujuan pemeriksaan sampel. 0=Default, 1=Suspek, 2=Kontak Erat, 3=Screening, 4=Pelaku perjalanan, 5=Alasan lain"
        },
        "tanggalGejala": {
          "type": "string",
          "description": "Tanggal terdeteksi gejala. dengan format yyyy-mm-dd",
          "example": "1982-03-12"
        },
        "nomorSpesimen": {
          "type": "string",
          "description": "Nomor spesimen dari lab yang menggunakan kode lab dari litbangkes",
          "example": "C.01.10312"
        },
        "nomorUrutSpesimen": {
          "type": "integer",
          "description": "Spesimen ini adalah pengambilan dari pasien yang kesekian kali"
        },
        "jenisSpesimen": {
          "type": "integer",
          "enum": [
            1,
            2,
            3,
            4,
            5,
            6,
            7,
            8
          ],
          "description": "Jenis spesimen yang diambil. 1=Swab Nasofaring, 2=Swab Orofaring, 3=Sputum, 4=Bal Fluid, 5=Cairan Spirasi Trakhea, 6=Serum, 7=Lainnya, 8=Swab Naso-Orofaring"
        },
        "tanggalPengambilan": {
          "type": "string",
          "description": "Tanggal pengambilan spesimen. dengan format yyyy-mm-dd",
          "example": "1982-03-12"
        },
        "statusPeriksa": {
          "type": "integer",
          "enum": [
            1,
            2
          ],
          "description": "Status pemeriksaan. 1=Belum Periksa, 2=Sudah Periksa"
        },
        "pembiayaan": {
          "type": "integer",
          "enum": [
            0,
            1,
            2
          ],
          "description": "Status pembiayaan. 0=Default, 1=Berbayar, 2=Tidak Berbayar"
        }
      }
    },
    "HasilSpesimen": {
      "required": [
        "hasil"
      ],
      "type": "object",
      "properties": {
        "tanggalHasil": {
          "type": "string",
          "description": "Tanggal hasil pemeriksaan sampel. dengan format yyyy-mm-dd",
          "example": "2021-01-12"
        },
        "hasil": {
          "type": "string",
          "description": "Hasil pemeriksaan",
          "enum": [
            "POSITIF",
            "NEGATIF"
          ]
        },
        "laboratorium": {
          "$ref": "#/definitions/NamaKode"
        }
      }
    },
    "DataFaskes": {
      "type": "object",
      "properties": {
        "faskes": {
          "$ref": "#/definitions/NamaKode"
        },
        "jenisFaskes": {
          "type": "integer",
          "enum": [
            1,
            2,
            3,
            4,
            5,
            6,
            7
          ],
          "description": "Jenis faskes yang memberikan laporan. 1=Puskesmas, 2=Rumah Sakit, 3=Dinkes Provinsi, 4=Dinkes Kabupaten Kota, 5=Klinik, 6=Laboratorium, 7=Lainnya"
        },
        "provinsi": {
          "$ref": "#/definitions/NamaKode"
        },
        "kabupatenKota": {
          "$ref": "#/definitions/NamaKode"
        }
      }
    },
    "NamaKodeJenis": {
      "type": "object",
      "allOf": [
        {
          "$ref": "#/definitions/NamaKode"
        }
      ],
      "properties": {
        "jenis": {
          "type": "string",
          "enum": [
            "PROVINSI",
            "KABKOT",
            "KECAMATAN",
            "KELURAHAN"
          ]
        }
      }
    },
    "NamaKodeJenisFaskes": {
      "type": "object",
      "allOf": [
        {
          "$ref": "#/definitions/NamaKode"
        }
      ],
      "properties": {
        "jenis": {
          "type": "string",
          "enum": [
            "RS",
            "Puskesmas"
          ]
        }
      }
    },
    "InfoLab": {
      "type": "object",
      "properties": {
        "lab": {
          "$ref": "#/definitions/NamaKodeLab"
        },
        "provinsi": {
          "$ref": "#/definitions/NamaKode"
        }
      }
    },
    "NamaKodeLab": {
      "type": "object",
      "allOf": [
        {
          "$ref": "#/definitions/NamaKode"
        }
      ],
      "properties": {
        "type_lab": {
          "type": "integer",
          "enum": [
            1,
            2
          ],
          "description": "0=Lab, 1=Lab RS"
        },
        "is_check": {
          "type": "integer",
          "enum": [
            1,
            2
          ],
          "description": "0=Tidak, 1=Ya"
        }
      }
    },
    "NamaKode": {
      "type": "object",
      "properties": {
        "nama": {
          "type": "string"
        },
        "kode": {
          "type": "string"
        }
      }
    }
  },
  "externalDocs": {
    "description": "NAR main website",
    "url": "https://allrecord-tc19.kemkes.go.id/"
  }
}